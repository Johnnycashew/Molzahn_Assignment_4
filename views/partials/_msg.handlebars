{{#if success_msg}}
<div class="alert alert-success alert-dismmissible mt-3">
    {{success_msg}}
    <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span
            area-hidden="true">&times;</span></button>
</div>
{{/if}}

{{#if error_msg}}
<div class="alert alert-danger alert-dismmissible mt-3">
    {{error_msg}}
    <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span
            area-hidden="true">&times;</span></button>
</div>
{{/if}}